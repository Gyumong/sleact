{
    "compilerOptions": {
      "esModuleInterop": true,  // import * as React from 'react';
      "sourceMap": true,
      "lib": ["ES2020", "DOM"],
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "Node",
      "target": "es5",
      "strict": true,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {                // 절대경로 
        "@hooks/*": ["hooks/*"],
        "@components/*": ["components/*"],
        "@layouts/*": ["layouts/*"],
        "@pages/*": ["pages/*"],
        "@utils/*": ["utils/*"],
        "@typings/*": ["typings/*"]
      }
    }
  }
  